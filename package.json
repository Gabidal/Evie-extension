{
    "name": "evie",
    "displayName": "Evie",
    "description": "Evie syntax highlighting",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "evie",
            "aliases": ["Evie", "evie"],
            "extensions": ["e"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "evie",
            "scopeName": "source.Evie",
            "path": "./syntaxes/evie.tmLanguage.json"
        }]
    }
}